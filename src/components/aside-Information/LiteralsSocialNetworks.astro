---
interface Props {
    url: string;
}

const { url } = Astro.props;
---
<li>
    <a class="group p-1 w-full rounded-lg bg-teal-400/30 block hover:bg-teal-400/60 transition-colors duration-300 ease-in-out" href={url}
        target="_blank" rel="noopener noreferrer">
        <slot/>
    </a>
</li>